{
  "name": "base-convert",
  "displayName": "Base Convert",
  "description": "这是一个vscode的进制转换悬浮提示插件",
  "publisher": "qmcx",
  "author": {
    "name": "青明尘下",
    "email": "qmcx.ming@qq.com"
  },
  "keywords": [
    "进制转换",
    "base-convert",
    "悬浮提示"
  ],
  "version": "1.0.2",
  "engines": {
    "vscode": "^1.90.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "images/icon.png",
  "activationEvents": [
    "*"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "baseConvert.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "baseConvert.copyTextCommand",
        "title": "复制文本"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@types/vscode": "^1.90.0",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5"
  }
}
